<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/sleepEditCategoryLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Category: "
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="24dp" />

    <TextView
        android:id="@+id/sleepEditCategory"
        android:layout_width="308dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="24dp" />

    <TextView
        android:id="@+id/sleepEditDateLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date: "
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="761dp" />

    <EditText
        android:id="@+id/sleepEditDate"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:inputType="date"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="731dp" />

    <TextView
        android:id="@+id/sleepEditStartTimeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time: "
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="761dp" />

    <EditText
        android:id="@+id/sleepEditStartTime"
        android:layout_width="221dp"
        android:layout_height="wrap_content"
        android:inputType="time"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="731dp" />

    <TextView
        android:id="@+id/sleepEditStopTimeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stop Time: "
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="761dp" />

    <EditText
        android:id="@+id/sleepEditStopTime"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:inputType="time"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="731dp" />

    <TextView
        android:id="@+id/sleepEditNotesLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notes: "
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="761dp" />

    <EditText
        android:id="@+id/sleepEditNotes"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="731dp" />

</androidx.constraintlayout.widget.ConstraintLayout>